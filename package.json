  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "Time",
    "Bloc",
    "Timebloc"
  ],
  "author": "",
  "license": "ISC",
  "homepage": "",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.5"
  },
  "files": [
    "lib/**/*"
  ]
